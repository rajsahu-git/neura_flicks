            if (document.getElementById("ContentPlaceHolder1_txtReportingtoID").value == "") {
                alert("Enter Manager ID");
                document.getElementById("ContentPlaceHolder1_txtReportingtoID").focus();
                return false;
            }
            if (document.getElementById("ContentPlaceHolder1_txtReportingtoName").value == "") {
                alert("Please enter correct Manager employee code.");
                document.getElementById("ContentPlaceHolder1_txtReportingtoID").focus();
                return false;
            }
            if (document.getElementById("ContentPlaceHolder1_txtdateofjoin").value == "") {
