                document.getElementById("ContentPlaceHolder1_txtdateofjoin").focus();
                return false;
            }
            var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds          
            var firstDate = new Date(document.getElementById("ContentPlaceHolder1_HiddenField10").value);
            var finaldate = firstDate.getDate() + '/' + (firstDate.getMonth() + 1) + '/' + firstDate.getFullYear();
            var secondDate = new Date(document.getElementById("ContentPlaceHolder1_txtdateofjoin").value);
            var diffDays = Math.round((firstDate.getTime() - secondDate.getTime()) / (oneDay));
